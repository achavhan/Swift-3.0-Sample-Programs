var n:Int=0
var sum:Int=0
var temp:Int=0
var r:Int=0

n=153

temp = n;
          while temp != 0 
          {
 
             r = temp%10;
             sum = sum + r*r*r;
             temp = temp/10; 
 
          }

 if  n == sum {
             print("\(n) is an Armstrong number.");
 } else {
             print("\(n) is not an Armstrong number.");  
 }
